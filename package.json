  },
  "homepage": "https://github.com/TucsonReactJS/primer",
  "dependencies": {
    "react": "^0.13.2",
    "snabbt.js": "^0.5.1",
    "superagent": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^5.2.16",
    "babel-core": "^5.1.11",
    "babel-loader": "^5.0.0",
    "browser-sync": "^2.6.4",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-babel": "^5.1.0",
    "gulp-load-plugins": "^0.10.0",
